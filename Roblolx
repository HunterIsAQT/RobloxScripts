local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("V2ly Gui", "Synapse")


    -- MAIN


local Main = Window:NewTab("Main")
local MainSection = Main:NewSection("Main")


MainSection:NewButton("Chat Spy", "Lets you see private messages", function()
    loadstring(game:HttpGet('https://pastebin.com/raw/wEf4SuiE'))()
end)

MainSection:NewButton("Cinematic Camera", "Press Shift + P to go into free cam", function()
    loadstring(game:HttpGet('https://pastebin.com/raw/p7jmfsQT'))()
end)

MainSection:NewButton("Infinite Yeild", "Admin Commands", function()
    loadstring(game:HttpGet('https://pastebin.com/raw/a2235uWK'))()
end)

MainSection:NewButton("Avatar Animations", "Changes your avatar animation R15 ONLY", function()
    loadstring(game:HttpGet('https://pastebin.com/raw/CXGg8sRP'))()
end)

MainSection:NewButton("Anti Afk", "Changes your avatar animation R15 ONLY", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/Proxylol/OtherScripts/main/AntiAfk.lua'))()
end)

MainSection:NewButton("Auto Clicker", "A Gui that lets you auto click", function()
    loadstring(game:HttpGet('loadstring(game:HttpGetAsync("https://raw.githubusercontent.com/JustEzpi/ROBLOX-Scripts/main/ROBLOX_AutoClicker"))()'))()
end)

-- PLAYER


local Player = Window:NewTab("Player")
local PlayerSection = Player:NewSection("Player")

PlayerSection:NewSlider("Walkspeed", "Changes how fast you walk", 500, 16, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)

PlayerSection:NewSlider("JumpHeight", "Changes how high you jump", 350, 50, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
end)

PlayerSection:NewSlider("Gravity", "SliderInfo", 500, 0, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Workspace.Gravity = s
end)

PlayerSection:NewButton("Reset WS/JP/GRAV", "Resets your walkspeed, jumpower, and gravity", function()
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
    game.Workspace.Gravity = 196.2
end)

PlayerSection:NewButton("Toggable fly", "Press e to fly", function()
    loadstring(game:HttpGet('https://pastebin.com/raw/6rY7DKPA'))()
end)

PlayerSection:NewButton("Wall Walk", "Lets you walk on walls", function()
    loadstring(game:HttpGet('https://pastebin.com/raw/15PCfhHT'))()
end)

PlayerSection:NewButton("Infinite Jump", "Lets you jump to infinity and beyond", function()
    loadstring(game:HttpGet('https://https://pastebin.com/raw/V3DKCQTT'))()
end)

PlayerSection:NewButton("R-15 FE invisiblity", "Makes you invisible to everybody but yourself", function()
    loadstring(game:HttpGet('https://pastebin.com/raw/hbiPAhqt'))()
end)


-- Teleport 


local Teleport = Window:NewTab("Teleport")
local TeleportSection = Teleport:NewSection("Teleport Player")


TeleportSection:NewTextBox("Teleport To Player", "Input Players name to teleport", function(txt)
	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players[txt].Character.HumanoidRootPart.CFrame
end)

TeleportSection:NewButton("Click Tp", "Press CTRL + M1 to teleport", function()
    loadstring(game:HttpGet('https://pastebin.com/raw/qyww70GR'))()
end)

